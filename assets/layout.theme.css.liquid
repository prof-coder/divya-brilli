.customer_result {
    display: flex;
    justify-content: space-evenly;
  	padding-top: 10px;
}
.proven_result h2 {
    text-align: center;
}
.full_width1 {
    background-color: #FFDA00;
    padding: 40px 0 20px 0;
}
.full_width_blue {
    background-color: #89D8E9;
    padding: 40px 0;
}
.result h2 {
    margin-top: 30px;
}
.metricpage-width {
    width: 1000px;
    margin: 40px auto;
}
  .metrics {
    width: 100%;
    display: flex;
    flex-flow: row wrap;
    align-items: baseline;
}
  .metric_part {
    width: 25%;
    display: flex;
    flex-flow: column wrap;
}
  .metric_part img {
    width: 85px;
    margin: 0 auto;
}
  .metric_part p {
    text-align: center;
    margin-top: 7px;
}
 .feature_part {
    width: 33.3333%;
    display: flex;
    flex-flow: column wrap;
}
  .feature_part img {
    width: 85px;
    margin: 0 auto;
}
  .feature_part p {
    text-align: center;
    margin-top: 7px;
}
  .features {
    width: 100%;
    display: flex;
    flex-flow: row wrap;
    align-items: baseline;
    max-width: 65%;
    margin: 0 auto;
}
.aa h2, .aa p {
    text-align: center;
}
  .aa h2 {
    margin-bottom: 30px;
    margin-top: 60px;
    padding: 5px;
}
.ScientificAdvisors {
    margin-top: 0 !important;
}
p.yellow_text, p.blue_text {
    text-align: center;
    color: #000;
  	margin-top: 50px;
    font-style: italic;
    margin-bottom: 0;
}
@media only screen and (max-width: 767px) {
   .metricpage-width {
    width: 100%;
}
  .metric_part {
    width: 50%;
  } 
  .feature_part img {
    width: 70%;
}
  .features {
    max-width: 88%;
}
  .result small {
    font-size: 17px;
}
  p.yellow_text, p.blue_text {
    font-size: 14px;
    margin-top: 40px;
}
}
